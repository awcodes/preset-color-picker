{
    "name": "awcodes/preset-color-picker",
    "description": "A color picker field for Filament Forms that uses preset color palettes.",
    "keywords": [
        "awcodes",
        "filament",
        "preset-color-picker",
        "plugin",
        "color-picker"
    ],
    "homepage": "https://github.com/awcodes/preset-color-picker",
    "support": {
        "issues": "https://github.com/awcodes/preset-color-picker/issues",
        "source": "https://github.com/awcodes/preset-color-picker"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Weston",
            "email": "awcodes1@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "filament/forms": "^3.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9|^8.1",
        "orchestra/testbench": "^8.0|^9.0",
        "pestphp/pest": "^2.1",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "pestphp/pest-plugin-livewire": "^2.1",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "Awcodes\\PresetColorPicker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Awcodes\\PresetColorPicker\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "test": "vendor/bin/pest --parallel",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Awcodes\\PresetColorPicker\\PresetColorPickerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
